{
  "name": "messenger-app",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Korneev",
  "repository": {
    "type": "git",
    "url": "git://github.com/Dm1Korneev/messenger-app.git"
  },
  "engines": {
    "node": "~10.15.1",
    "npm": "~6.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "aws-sdk": "^2.463.0",
    "classnames": "^2.2.6",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.5.11",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-material-ui-form-validator": "^2.0.8",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.0.2",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "nodemon": "^1.19.1",
    "react-scripts": "^3.1.1",
    "source-map-explorer": "^2.0.1"
  },
  "scripts": {
    "start": "start mongod '--config ./mongod.cfg' && start nodemon ./bin/www && start react-scripts start",
    "react-start": "react-scripts start",
    "build": "react-scripts build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-nodemon": "nodemon ./bin/www",
    "start-mongod": "mongod --config ./mongod.cfg",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src/ --ext .jsx,.js --color",
    "lint-fix": "eslint src/ --ext .jsx,.js --color --fix"
  },
  "proxy": "http://localhost:8080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "app_api/*",
      "bin/*",
      "server.js"
    ],
    "delay": "2500"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "git add"
    ]
  }
}
