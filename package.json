{
  "name": "messenger-app",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Korneev",
  "repository": {
    "type": "git",
    "url": "git://github.com/Dm1Korneev/messenger-app.git"
  },
  "engines": {
    "node": "~10.15.1",
    "npm": "~6.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.4",
    "@material-ui/icons": "^3.0.2",
    "aws-sdk": "^2.663.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.3",
    "http-errors": "^1.7.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-material-ui-form-validator": "^2.0.10",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "git-branch-is": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "nodemon": "^2.0.3",
    "react-scripts": "^3.4.1",
    "source-map-explorer": "^2.4.2"
  },
  "scripts": {
    "start": "start mongod '--config ./mongod.cfg' && start nodemon ./bin/www && start react-scripts start",
    "start-react": "react-scripts start",
    "start-nodemon": "nodemon ./bin/www",
    "start-mongod": "mongod --config ./mongod.cfg",
    "build": "react-scripts build",
    "heroku-postbuild": "npm run build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint src/ app_api/ bin/www server.js --ext .jsx,.js --color",
    "lint-fix": "eslint src/ app_api/ bin/www server.js --ext .jsx,.js --color --fix",
    "test-all": "react-scripts test --watchAll=false"
  },
  "proxy": "http://localhost:8080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "app_api/*",
      "bin/*",
      "server.js"
    ],
    "delay": "2500"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run test-all"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --ext .jsx,.js --color"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/index.jsx",
      "/src/serviceWorker.js"
    ]
  }
}
