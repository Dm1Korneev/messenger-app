{
  "name": "messenger-app",
  "version": "0.1.0",
  "private": true,
  "author": "Dmitriy Korneev",
  "engines": {
    "node": "~10.15.1",
    "npm": "~6.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.16.0",
    "express-jwt": "^5.3.1",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.13",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5"
  },
  "scripts": {
    "start-server": "node ./bin/www",
    "start-mongo": "mongod",
    "start-client": "react-scripts start",
    "start": "start mongod && start nodemon ./bin/www && start react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-and-start-express": "npm run start-server && npm run build"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "app_api/*",
      "bin/*",
      "server.js"
    ],
    "delay": "2500"
  }
}
